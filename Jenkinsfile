pipeline {
    agent any
    stages {
        stage ('Check Dependencies'){
            steps{
                echo 'Checking Dependencies'
            }
        }
        stage ('Unit Tests'){
            steps{
                echo 'Running Unit Tests'
            }
        }
        stage ('Build'){
            steps{
                echo 'Build'
            }
        }
        stage ('Deploy'){
            steps{
                echo 'Deploying'
            }
        }
    }
}


